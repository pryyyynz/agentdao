# AI/ML - Groq API
GROQ_API_KEY=your_groq_api_key_here

# Blockchain - Thirdweb
THIRDWEB_SECRET_KEY=your_thirdweb_secret_key_here
THIRDWEB_CLIENT_ID=your_thirdweb_client_id_here

# Blockchain - RPC Provider (Infura or Alchemy)
RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Blockchain - Etherscan API (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Wallet (Testnet only! Never use mainnet keys)
PRIVATE_KEY=0x_your_private_key_here

# Database - Supabase PostgreSQL (connection pooler - recommended)
DATABASE_URL=postgresql://postgres.PROJECT_ID:PASSWORD@aws-0-REGION.pooler.supabase.com:6543/postgres
DB_MIN_CONNECTIONS=1
DB_MAX_CONNECTIONS=10
DB_CONNECTION_TIMEOUT=30

# Supabase
SUPABASE_URL=https://PROJECT_ID.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# IPFS (Pinata)
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_API_KEY=your_pinata_secret_key_here
PINATA_JWT=your_pinata_jwt_here

# GitHub API (for due diligence checks)
GITHUB_API_KEY=ghp_your_github_token_here

# Service URLs
PYTHON_SERVICE_URL=http://localhost:8000
MCP_SERVER_URL=http://localhost:3100

# Environment
ENVIRONMENT=development

# Email Service (Resend)
RESEND_API_KEY=your_resend_api_key_here
RESEND_FROM_EMAIL=your_email@example.com

# JWT Authentication
JWT_SECRET_KEY=your-secret-key-change-in-production
